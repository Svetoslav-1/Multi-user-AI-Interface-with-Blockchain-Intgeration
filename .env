# .env - Environment variables for the application
# Server configuration
PORT=5000
NODE_ENV=development

# JWT Secret - IMPORTANT: Change this to a strong random value in production
JWT_SECRET=your-jwt-secret-here

# Blockchain configuration
BLOCKCHAIN_PROVIDER_URL=blockchain_provider
CONTRACT_ADDRESS=0x1234567890123456789012345678901234567890

# AI Model configuration
# Uncomment and populate the relevant API key for your chosen AI provider

# OpenAI Configuration
# OPENAI_API_KEY=your-openai-api-key

# Anthropic Configuration
# ANTHROPIC_API_KEY=your-anthropic-key

# Cors Configuration
CORS_ORIGIN=*

# Database configuration (for future implementation)
# DB_HOST=localhost
# DB_PORT=5432
# DB_NAME=multi_user_ai
# DB_USER=postgres
# DB_PASSWORD=password
